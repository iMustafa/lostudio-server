{
  "name": "Dashboard",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AutoSetUser": true,
    "OwnerRestrictedStaticMethods": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    },
    "widgetSettings": {
      "type": "hasMany",
      "model": "WidgetSettings",
      "foreignKey": "",
      "through": "DashboardWidgetSettings"
    },
    "dataSource": {
      "type": "belongsTo",
      "model": "DataSource",
      "foreignKey": ""
    },
    "dashboardRoleMappings": {
      "type": "hasMany",
      "model": "DashboardRoleMapping",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "_find"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "collaborations"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "_count"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "dashboardEditor",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "dashboardViewer",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
